{
  "npmScope": "esc",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "admin": {
      "tags": []
    },
    "admin-e2e": {
      "tags": [],
      "implicitDependencies": ["admin"]
    },
    "api": {
      "tags": []
    },
    "product-api": {
      "tags": []
    },
    "product-domain": {
      "tags": ["domain:product", "type:domain-logic"]
    },
    "product-entities": {
      "tags": []
    },
    "shop": {
      "tags": []
    },
    "shop-e2e": {
      "tags": [],
      "implicitDependencies": ["shop"]
    },
    "user-api": {
      "tags": []
    },
    "user-domain": {
      "tags": ["domain:user", "type:domain-logic"]
    },
    "user-entities": {
      "tags": []
    }
  }
}
